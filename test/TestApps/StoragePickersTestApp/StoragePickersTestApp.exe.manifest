      name="Microsoft.Windows.Storage.Pickers.FileSavePicker"
      threadingModel="both"
      xmlns="urn:schemas-microsoft-com:winrt.v1" />
    <activatableClass
      name="Microsoft.Windows.Storage.Pickers.FolderPicker"
      threadingModel="both"
      xmlns="urn:schemas-microsoft-com:winrt.v1" />
    <activatableClass
      name="Microsoft.Windows.Storage.Pickers.PickFileResult"
      threadingModel="both"
      xmlns="urn:schemas-microsoft-com:winrt.v1" />
    <activatableClass
      name="Microsoft.Windows.Storage.Pickers.PickFolderResult"
      threadingModel="both"
      xmlns="urn:schemas-microsoft-com:winrt.v1" />
  </file>

</assembly>