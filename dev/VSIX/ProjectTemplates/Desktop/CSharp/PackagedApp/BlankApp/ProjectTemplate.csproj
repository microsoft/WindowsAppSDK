<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net5.0-windows10.0.19041.0</TargetFramework>
    <TargetPlatformMinVersion>10.0.17763.0</TargetPlatformMinVersion>
    <RootNamespace>$safeprojectname$</RootNamespace>
    <ApplicationManifest>app.manifest</ApplicationManifest>
    <Platforms>x86;x64;arm64</Platforms>
    <RuntimeIdentifiers>win10-x86;win10-x64;win10-arm64</RuntimeIdentifiers>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.ProjectReunion" Version="$ext_ReunionNupkgVersion$" />
    <PackageReference Include="Microsoft.ProjectReunion.Foundation" Version="$ext_ReunionFoundationNupkgVersion$" />
    <PackageReference Include="Microsoft.ProjectReunion.WinUI" Version="$ext_ReunionWinUINupkgVersion$" />
    <Manifest Include="$(ApplicationManifest)" />
  </ItemGroup>
</Project>
