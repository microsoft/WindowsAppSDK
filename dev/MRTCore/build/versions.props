    <MicrosoftSourceLinkAzureReposVersion>1.0.0</MicrosoftSourceLinkAzureReposVersion>
    <VCRTForwardersVersion>1.0.6</VCRTForwardersVersion>
    <WebView2Version>0.9.573-internal</WebView2Version>
    <VisualStudio166PatchVersion>1.0.6</VisualStudio166PatchVersion>
  </PropertyGroup>
  <!-- Package names -->
  <PropertyGroup>
    <WindowsSdkNugetPackageName>Microsoft.Windows.SDK</WindowsSdkNugetPackageName>
    <WinUIDetailsPackageName>Microsoft.Internal.WinUIDetails</WinUIDetailsPackageName>
    <LiftedIXPPackageName>CompInproc</LiftedIXPPackageName>
