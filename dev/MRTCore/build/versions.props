    <TaefNugetPackageVersion>10.52.200221001</TaefNugetPackageVersion>
    <DotNetFrameworkVersion>v4.6.1</DotNetFrameworkVersion>
    <DotNetTargetFrameworkMoniker>net46</DotNetTargetFrameworkMoniker>
    <DotNetCoreTargetFrameworkMoniker>net5.0</DotNetCoreTargetFrameworkMoniker>
    <WindowsTargetPlatformVersion>10.0.18362.0</WindowsTargetPlatformVersion>
    <WindowsTargetPlatformMinVersion>10.0.18362.0</WindowsTargetPlatformMinVersion>
